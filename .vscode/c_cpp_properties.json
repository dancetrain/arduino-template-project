{
    "env": {
        "arduino.path": "/Applications/Arduino.app/Contents/Java",
        "arduino.avr.include.path": "${env:arduino.path}/hardware/arduino/avr",
        "arduino.avr.include2.path": "${env:arduino.path}/hardware/tools/avr/include",
        "arduino.avr.compiler.path": "${env:arduino.path}/hardware/tools/avr/bin/avr-g++",
        "arduino.libraries.path": "${HOME}/Documents/Arduino/libraries",
        "dummy-last-line": "To allow the second to last line (e.g. the real last line) to always end with a comma"
    },
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${env:arduino.path}/tools/**",
                "${env:arduino.avr.include.path}/**",
                "${env:arduino.avr.include2.path}/**",
                "${env:arduino.libraries.path}/FastLED/**",
                "/Applications/Arduino.app/Contents/Java/hardware/arduino/avr/**",
                "/Applications/Arduino.app/Contents/Java/tools/**"
            ],
            "forcedInclude": [
                "/Applications/Arduino.app/Contents/Java/hardware/arduino/avr/cores/arduino/Arduino.h"
            ],
            "macFrameworkPath": [
                "/System/Library/Frameworks",
                "/Library/Frameworks"
            ],
            "intelliSenseMode": "clang-x64",
            "compilerPath": "${env:arduino.avr.compiler.path}",
            "cStandard": "c18",
            "cppStandard": "c++17",
            "defines": [
                "USBCON"
            ]
        }
    ],
    "version": 4
}